{
  "name": "example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:build": "tsc && docker build -t hanpama/c2g .",
    "docker:test": "docker run --rm hanpama/c2g npm test",
    "docker:convert": "tsc && docker run --rm -v $(pwd)/build:/var/app/build -v $(pwd)/kmzmodels:/var/kmzmodels -v $(pwd)/browser/public/data:/var/export hanpama/c2g node build/src/convert_kmzmodels.js",
    "build": "npm run docker:convert && cd browser && npm run build && cd ../ && rm -rf docs && mv browser/build docs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/graphql": "^0.13.4",
    "@types/graphql-relay": "^0.4.5",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^23.3.1",
    "@types/xml2js": "^0.4.3",
    "couchrelay": "^0.1.11",
    "girin": "^0.4.2",
    "graphql": "^0.13.2",
    "graphql-relay": "^0.5.5",
    "invariant": "^2.2.4",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1",
    "xml2js": "^0.4.19"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.ts",
      "**/test/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.34",
    "isomorphic-fetch": "^2.2.1"
  }
}
