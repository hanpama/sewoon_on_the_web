{
  "name": "example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:build": "tsc && docker build -t hanpama/c2g .",
    "docker:test": "docker run --rm hanpama/c2g npm test",
    "docker:convert": "tsc && docker run --rm -v $(pwd)/build:/var/app/build -v $(pwd)/kmzmodels:/var/kmzmodels -v $(pwd)/browser/public/data:/var/export hanpama/c2g node build/src/convert_kmzmodels.js",
    "build": "npm run docker:convert && cd browser && npm run build && cd ../ && rm -rf docs && mv browser/build docs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/xml2js": "^0.4.2",
    "apollo-server-express": "^1.2.0",
    "express": "^4.16.2",
    "graphql": "^0.11.7",
    "jest": "^21.2.1",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^3.1.13",
    "ts-jest": "^21.2.4",
    "typeorm": "^0.1.7",
    "typescript": "^2.6.2",
    "xml2js": "^0.4.19"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.ts",
      "**/test/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
