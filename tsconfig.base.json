{
  "version": "3.0.1",
  "compilerOptions": {
      "lib": ["es5", "es6", "esnext.asynciterable", "dom"],
      "target": "es2016",
      "module": "commonjs",
      "moduleResolution": "node",
      "allowJs": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "noImplicitAny": true,
      "declaration": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitReturns": true,
      "stripInternal": true,
      "pretty": true,
      "strictNullChecks": true,
      "noUnusedLocals": true
  }
}